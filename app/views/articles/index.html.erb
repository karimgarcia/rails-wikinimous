<h1>Articles#index</h1>
<p>Find me in app/views/articles/index.html.erb</p>
<%= link_to 'Create an article', new_article_path %>
<ul>
<% @articles.each do |article| %>
<li><%= article.title %><%= article.content %>
<div><%= link_to '[EDIT]', edit_article_path(article) %> - <%= link_to '[SHOW]', article_path(article) %> - <%= link_to '[DELETE]', article_path(article), method: :delete, data: { confirm: "Are you sure?" } %></div>
</li>

<% end %>
</ul>
